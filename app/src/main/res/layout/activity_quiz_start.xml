<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.katie.storyseeker.QuizStart">

    <ImageButton
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:src="@drawable/season"
        android:scaleType="fitXY"
        android:background="@null"
        android:onClick="seasons"
        android:id="@+id/seasons"
        android:paddingLeft="20dp"
        android:layout_above="@+id/thing3"
        android:layout_alignParentStart="true" />
    <ImageView
        android:layout_width="275dp"
        android:layout_height="275dp"
        android:src="@drawable/puppy"
        android:scaleType="fitXY"
        android:background="@null"
        android:id="@+id/thing2"
        android:paddingLeft="20dp"
        android:layout_marginTop="123dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/thing4" />
    <ImageView
        android:layout_width="275dp"
        android:layout_height="275dp"
        android:scaleType="fitXY"
        android:src="@drawable/puppy"
        android:background="@null"
        android:paddingLeft="20dp"
        android:id="@+id/thing3"
        android:layout_below="@+id/thing2" />
    <ImageView
        android:layout_width="275dp"
        android:layout_height="275dp"
        android:scaleType="fitXY"
        android:src="@drawable/puppy"
        android:paddingLeft="20dp"
        android:background="@null"
        android:id="@+id/thing4"
        android:layout_below="@+id/thing2"
        android:layout_toEndOf="@+id/thing3" />


</RelativeLayout>
